{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://webstormtroopers.herokuapp.com/login.html"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "img"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Dear user, log in to access the admin area"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "i.entypo-user"
      }
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "id",
        "value": "username"
      },
      "value": ""
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "value": ""
    },
    {
      "type": "verifyTextPresent",
      "text": "Login"
    },
    {
      "type": "verifyTextPresent",
      "text": "Forgot password"
    },
    {
      "type": "verifyTextPresent",
      "text": "ToS"
    },
    {
      "type": "verifyTextPresent",
      "text": "Privacy Policy"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}