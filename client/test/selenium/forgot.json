{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://webstormtroopers.herokuapp.com/forgot-password.html"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "img"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Enter your email, and we will send the reset link"
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "id",
        "value": "email"
      },
      "value": ""
    },
    {
      "type": "verifyTextPresent",
      "text": "Send Reset Link"
    },
    {
      "type": "verifyTextPresent",
      "text": "Return to Login Page"
    },
    {
      "type": "verifyTextPresent",
      "text": "ToS"
    },
    {
      "type": "verifyTextPresent",
      "text": "Privacy Policy"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}